{
  "name": "@sunxiao79/muse-ui",
  "version": "3.0.7",
  "description": "material design ui for vue2",
  "author": "Sun79 <1652829889@qq.com>",
  "repository": "https://github.com/Sun79/muse-ui",
  "main": "dist/@sunxiao79/muse-ui.common.js",
  "module": "dist/@sunxiao79/muse-ui.esm.js",
  "unpkg": "dist/@sunxiao79/muse-ui.js",
  "jsdelivr": "dist/@sunxiao79/muse-ui.js",
  "typings": "types/index.d.ts",
  "files": [
    "dist",
    "lib",
    "es5",
    "src",
    "types"
  ],
  "scripts": {
    "dev": "cross-env NODE_ENV=normal parcel demo/index.html",
    "build": "npm run build:dist && npm run build:lib && npm run build:es5",
    "build:dist": "rimraf dist && rollup --config rollup.config.js",
    "build:lib": "rimraf lib && cross-env NODE_ENV=normal babel src -d lib --ignore *.spec.js && ncp src/styles lib/styles",
    "build:es5": "rimraf es5 && cross-env NODE_ENV=es5 babel src -d es5 --ignore *.spec.js && ncp src/styles es5/styles",
    "lint": "cross-env NODE_ENV=normal eslint --ext .js,.vue src",
    "test": "cross-env NODE_ENV=test jest -i",
    "test:coverage": "cross-env NODE_ENV=test jest -i --coverage",
    "prepush": "yarn run lint",
    "commit": "npx git-cz",
    "commitmsg": "commitlint -E GIT_PARAMS"
  },
  "peerDependencies": {
    "vue": "^2.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-conventional": "^7.0.1",
    "@vue/component-compiler-utils": "^3.0.0",
    "autoprefixer": "^9.6.1",
    "avoriaz": "^6.3.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-jest": "^22.4.3",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-transform-vue-jsx": "^3.5.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5.2.0",
    "cssnano": "^4.1.10",
    "eslint": "^4.18.1",
    "eslint-config-vue": "^2.0.2",
    "eslint-plugin-vue": "^4.3.0",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "jest-cli": "^22.4.3",
    "jest-css-modules": "^1.1.0",
    "jest-serializer-html": "^5.0.0",
    "less": "^3.0.1",
    "ncp": "^2.0.0",
    "parcel-bundler": "^1.9.7",
    "postcss-import": "^11.0.0",
    "postcss-modules": "^1.1.0",
    "postcss-url": "^7.2.1",
    "rimraf": "^2.6.0",
    "rollup": "^0.57.1",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-postcss": "^1.4.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^3.0.0",
    "typeface-roboto": "^0.0.54",
    "vue": "^2.5.13",
    "vue-hot-reload-api": "^2.3.3",
    "vue-jest": "^2.3.0",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "^2.5.13"
  },
  "dependencies": {
    "body-scroll-lock": "^2.6.1",
    "dayjs": "^1.8.2",
    "keycode": "^2.1.9",
    "normalize-wheel": "^1.0.1",
    "resize-observer-polyfill": "^1.5.0"
  },
  "browserslist": [
    "> 1%",
    "last 5 versions",
    "ios >= 7",
    "android > 4.4",
    "not ie < 10"
  ]
}
